<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean class="${package}.${artifactId-safe}.rest.ModuleService" />

  <bean class="${package}.${artifactId-safe}.model.converter.TransportToExternalConverter" />

  <bean class="${package}.${artifactId-safe}.model.converter.ExternalToTransportConverter" />

  <bean class="${package}.${artifactId-safe}.validator.Validator" />

  <bean class="${package}.${artifactId-safe}.model.converter.ExternalToInternalConverter" />

  <bean class="${package}.${artifactId-safe}.model.converter.InternalToExternalConverter" />

  <bean class="${package}.${artifactId-safe}.pdf.PdfGenerator">
    <constructor-arg value="true" />
  </bean>

  <bean class="${package}.${artifactId-safe}.model.converter.WebcertModelFactory" />

  <bean id="${artifactId}EntryPoint"
    class="${package}.${artifactId-safe}.support.TsBasEntryPoint" />
</beans>